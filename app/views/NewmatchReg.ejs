<div class="container newmatch-section">
    <div class="row">
         <form class="form-horizontal form-content" action="" method="" enctype="multipart/form-data">
         
                <h3>New Match Registration</h3>

                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 border-secondary border-right">
                        <div class="row">
                            <label class="col-lg-4 col-md-4 col-sm-12 col-xs-12">Tournament Name:<span class="mandatory-field">*</span></label>
                            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                <select required id="tournamentName" name="tournamentName" class="input-xlarge form-control">
                                    <option value="">--Select--</option>
                                    <%= tournamentList[0] %>
                                    <% for(var i=0; i<tournamentList[0].length; i++) {%>
                                       <%if (tournamentList[0][i].tournament_name.length > 0) { %>
                                            <option value=<%= tournamentList[0][i].tournament_id %>><%= tournamentList[0][i].tournament_name %></option>
                                               
                                       <% } %>
                                    <% } %>
                                </select>
                            </div>
                        </div>

                        <div class="row">
                            <label class="col-lg-4 col-md-4 col-sm-12 col-xs-12">Team 1:<span class="mandatory-field">*</span></label>
                            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                <select required id="teamOne" name="teamOne" class="input-xlarge form-control">
                                    <option value="">--Select--</option>
                                    <% for(var i=0; i<teamsList[0].length; i++) {%>
                                       <%if (teamsList[0][i].team_code.length > 0) { %>
                                            <option value=<%= teamsList[0][i].team_code %>><%= teamsList[0][i].team_name %></option>
                                               
                                       <% } %>
                                    <% } %>
                                </select>
                            </div>
                        </div>

                        <div class="row">
                            <label class="col-lg-4 col-md-4 col-sm-12 col-xs-12">Team 2:<span class="mandatory-field">*</span></label>
                            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                <select required id="teamtwo" name="teamtwo" class="input-xlarge form-control">
                                    <option value="">--Select--</option>
                                    <% for(var i=0; i<teamsList[0].length; i++) {%>
                                       <%if (teamsList[0][i].team_code.length > 0) { %>
                                            <option value=<%= teamsList[0][i].team_code %>><%= teamsList[0][i].team_name %></option>
                                               
                                       <% } %>
                                    <% } %>
                                </select>
                            </div>
                        </div>

                        <!-- <div class="row">
                            <label class="col-lg-4 col-md-4 col-sm-12 col-xs-12">Team 2:</label>
